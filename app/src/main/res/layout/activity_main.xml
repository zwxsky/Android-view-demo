<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <TextView
        android:id="@+id/text_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="hello_world"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"/>

    <TextView
        android:id="@+id/edit_text"
        android:layout_below="@+id/text_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="EditText"
        android:textColor="@android:color/holo_green_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="true"

        android:textSize="20dp"
        android:onClick="showEditText"/>

    <TextView
        android:id="@+id/autoCompleteTextView"
        android:layout_below="@+id/edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="CompleteTextView"
        android:textColor="@android:color/holo_green_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="true"

        android:textSize="20dp"
        android:onClick="showAutoCompleteTextView"/>

    <TextView
        android:id="@+id/button"
        android:layout_below="@+id/autoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="button"
        android:textColor="@android:color/holo_purple"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showButton"/>

    <TextView
        android:id="@+id/imageButton"
        android:layout_below="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="imageButton"
        android:textColor="@android:color/holo_purple"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showImageButton"/>

    <TextView
        android:id="@+id/checkbox"
        android:layout_below="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="checkbox"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showCheckboxButton"/>


    <TextView
        android:id="@+id/toggleButton"
        android:layout_below="@+id/checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="toggleButton"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showToggleButton"/>


    <TextView
        android:id="@+id/datepicker"
        android:layout_below="@+id/toggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="datepicker"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showDatepicker"/>

    <TextView
        android:id="@+id/spinner"
        android:layout_below="@+id/datepicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="spinner 下拉选择框"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showSpinner"/>

    <TextView
        android:id="@+id/processBar"
        android:layout_below="@+id/spinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="processBar 进度条"
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showProcessBar"/>

    <TextView
        android:id="@+id/radioButton"
        android:layout_below="@+id/processBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="radioButton "
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"

        android:textSize="20dp"
        android:onClick="showRadioButton"/>

    <TextView
        android:id="@+id/marquee"
        android:layout_below="@+id/radioButton"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="aamarquee "
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"
        android:textSize="20dp"
        android:capitalize="characters"
        android:onClick="showMarqueeButton"/>

    <TextView
        android:id="@+id/showMultiAutoCompleteTextView"
        android:layout_below="@+id/marquee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="characters"
        android:text="text自动匹配text "
        android:textColor="@android:color/holo_blue_dark"
        android:textColorHighlight="@android:color/primary_text_dark"
        android:layout_centerVertical="false"
        android:textSize="20dp"
        android:onClick="showMultiAutoCompleteTextView"/>





</RelativeLayout>
